.main {
	box-sizing: border-box;
	height: 100%;
}

.main-team {
	width: 885px;
	float: right;
}
.nav-main__item,
.nav-main__item_active{
	line-height: 18px;
	margin-bottom: 30px;
}
.nav-main__href {
	text-decoration: none;
	color: #1c5bc3;
	font-family: Roboto;
	font-size: 12px;
	font-weight: 700;
	text-transform: uppercase;
	letter-spacing: 2.4px;
}
.nav-main__item:hover .nav-main__href,
.nav-main__item_active .nav-main__href{
	color: #221f1f;
}
.nav-main__item:before,
.nav-main__item_active:before
{
	content: "";
	display: block;
	width: 20px;
	height: 18px;
	float: left;
	margin-right: 16px;
	background-repeat:no-repeat;
	background-position: center;
	background-size: contain;
}
.nav-main__item_aircraft:hover:before,.nav-main__item_active:before{
	background-image: url(../img/icon/aircraft-hover.png);
}
.nav-main__item_aircraft:before{
	background-image: url(../img/icon/aircraft.png) ;
}
.nav-main__item_star:before{
	background-image: url(../img/icon/star.png) ;
}
.nav-main__item_star:hover:before,.nav-main__item_active:before{
	background-image: url(../img/icon/star-hover.png) ;
}
.nav-main__item_cap:before{
	width: 22px;
	background-image: url(../img/icon/cap.png);
}
.nav-main__item_cap:hover:before,.nav-main__item_active:before{
	background-image: url(../img/icon/cap-hover.png);
}

.bread-crumbs {
	margin-bottom: 18px;
}
.bread-crumbs__items {
	font-size: 14px;
	font-style: italic;
	font-weight: 300;
	line-height: 1;
	opacity: 0.8;
	text-indent: 2.4px;
}
.bread-crumbs__item,
.bread-crumbs__item_active {
	display: inline-block;
}
.bread-crumbs__item:not(:last-child):after{
	content: " / ";
}
.bread-crumbs__href {
	color: #5b96d9;
}
.bread-crumbs__item_active .bread-crumbs__href {
	text-decoration: none;
	color: #221f1f;
}
.title {
	color: #221f1f;
	font-family: Roboto;
	font-size: 49px;
	font-weight: 700;
	margin-bottom: 38px;
}
.crew {
	width: 900px;
	margin: auto;
}
.crew__items {
	/* text-align: center; */
	width: 912px;
}

.crew__item {
	width: 288px;
	display: inline-block;
	vertical-align: top;
	text-align: center;
	margin-right: 7px;
	margin-bottom: 42px;
}
.crew__ava {
	width: 160px;
	height: 160px;
	display: inline-block;
	border-radius: 50%;
	overflow: hidden;
	text-align: center;
	margin-bottom: 13px;
}

.crew__name {
	font-size: 18px;
	font-weight: 700;
	text-align: center;
	margin-bottom: 5px;
}

.crew__profession {
	color: #1c5bc3;
	font-weight: 700;
	text-align: center;
	font-size: 14px;
	margin-bottom: 8px;
}

.crew__description {
	font-weight: 300;
	display: inline-block;
}

.crew__ava-img {
	width: 160px;
}

.crew__content {
	width: 260px;
	text-align: center;
	font-size: 14px;
	line-height: 21px;
}
.crew__content_3{
	width: 256px;
